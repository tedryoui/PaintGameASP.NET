<!DOCTYPE html><html lang="en">
<head>
  <script src="~/lib/p5-js/p5.js"></script>
  <script src="~/js/Models/point.js"></script>
  <script src="~/js/Models/curve.js"></script>
  <script src="~/js/Models/sticky-note.js"></script>
  <script src="~/js/Models/text.js"></script>
  <link rel="stylesheet" type="text/css" href="~/css/site.css">
  <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css">
  <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.7.0/css/all.css" integrity="sha384-lZN37f5QGtY3VHgisS14W3ExzMWZxybE1SJSEsQp9S+oqd12jhcu+A56Ebc1zFSJ" crossorigin="anonymous">
  <meta charset="utf-8">

</head>
<body class="bg-info">
    
  
  <div class="position-absolute w-100 d-flex justify-content-center mt-3">
    <div class="col-3">
      <div class="row">
        <div class="input-group mb-3" id="textInputContainer">
        </div>
      </div>
    </div>
  </div>
  <div class="postition-absolute fixed-bottom w-100 d-flex justify-content-center" style="height: 80%">
    <div class="col-10 h-100">
      <div class="row h-100 w-100" id="canvasContainer">
      </div>
    </div>
  </div>
  <div class="position-absolute" style="z-index: 9999;">
    <button id="drawButton" class="btn border border-secondary bg-light rounded m-2 mb-0 mt-5" style="width: 50px; height: 50px;"><i class="fas fa-paint-brush"></i></button><br>
    <button id="textButton" class="btn border border-secondary bg-light rounded m-2 mb-0" style="width: 50px; height: 50px;"><i class="fas fa-font"></i></button><br>
    <button id="noteButton" class="btn border border-secondary bg-light rounded m-2 mb-0" style="width: 50px; height: 50px;"><i class="fas fa-sticky-note"></i></button><br>
    <button id="deleteButton" class="btn border border-secondary bg-light rounded m-2 mb-0" style="width: 50px; height: 50px;"><i class="fas fa-eraser"></i></button><br>
  </div>
  
  
  <script src="~/js/site.js"></script>
  
  <script src="~/lib/jquery/dist/jquery.min.js"></script>
  <script src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>
</body></html>